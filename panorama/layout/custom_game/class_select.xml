      </Button>
      <Button id="SelectCyborgButton" onactivate="SelectCyborgButtonPressed()">
         <Label text="Cyborg" />
      </Button>
      <Button id="SelectFieldMedicButton" onactivate="SelectFieldMedicButtonPressed()">
         <Label text="FieldMedic" />
